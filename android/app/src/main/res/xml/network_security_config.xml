<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <domain-config cleartextTrafficPermitted="true">
        <!-- For React Native Hot-reloading system -->
        <!-- If you are running on a device insert your computer IP -->
        <domain includeSubdomains="true">localhost</domain>
        <domain includeSubdomains="true">10.0.2.2</domain>
        <domain includeSubdomains="true">10.14.121.6</domain>
        <domain includeSubdomains="true">app.itel.vn</domain>
        <domain includeSubdomains="true">123.30.50.94</domain>
        <trust-anchors>
            <certificates src="system"/>
            <certificates src="@raw/itel"/>
            <certificates src="user"/>
        </trust-anchors>
    </domain-config>
    <base-config cleartextTrafficPermitted="true"/>
</network-security-config>
